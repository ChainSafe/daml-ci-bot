{
  "name": "daml-ci-bot",
  "version": "1.0.0",
  "private": true,
  "description": "DAML test bot",
  "author": "irubido",
  "license": "ISC",
  "homepage": "https://github.com//",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "start": "probot run ./lib/index.js",
    "dev": "./scripts/dev.sh",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src test --ext .ts,.js",
    "lint:fix": "eslint src test --ext .ts,.js --fix"
  },
  "dependencies": {
    "probot": "^13.4.5"
  },
  "devDependencies": {
    "@octokit/types": "^15.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^8.57.0",
    "nock": "^14.0.5",
    "smee-client": "^2.0.0",
    "typescript": "^5.8.3",
    "vitest": "^2.0.0"
  },
  "overrides": {
    "@types/pg": "8.15.1"
  },
  "engines": {
    "node": ">= 18"
  },
  "type": "module"
}
